	keywords = {forward kinematics, inverse kinematics, manipulator kinematics, Manipulators, Mathematical model, Jacobian matrices, 3-D force sensing, actuation-based force sensing, compliance control, Continuum robots, ellipsoids, end effectors, end-effector compliance, flexible links, Force, force application, force sensing, force sensors, force-sensing ability, generalized Cosserat-rod-based kinetostatic model framework, manipulator Jacobian, matrices, nondimensional analysis, parallel continuum robots, parallel robots, pose actuation, prototype six-degree-of-freedom {PCR}, rigid-link parallel robots, robot kinematics, Robot sensing systems},
	file = {IEEE Xplore Abstract Record:/home/andrea/Zotero/storage/IBZT8KSH/8207644.html:text/html;Black et al. - 2018 - Parallel Continuum Robots Modeling, Analysis, and.pdf:/home/andrea/Zotero/storage/WJ2QXK76/Black et al. - 2018 - Parallel Continuum Robots Modeling, Analysis, and.pdf:application/pdf},
